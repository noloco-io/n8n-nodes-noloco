{
	"name": "@noloco/n8n-nodes-noloco",
	"version": "1.0.0",
	"description": "n8n community node for Noloco - a no-code platform for building internal tools and client portals",
	"license": "MIT",
	"homepage": "https://noloco.io",
	"keywords": [
		"n8n-community-node-package",
		"noloco",
		"no-code",
		"internal-tools",
		"client-portals"
	],
	"author": {
		"name": "Noloco",
		"email": "support@noloco.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/noloco-io/n8n-nodes-noloco.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/NolocoApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Noloco/Noloco.node.js",
			"dist/nodes/Noloco/NolocoTrigger.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"packageManager": "pnpm@10.15.1"
}
